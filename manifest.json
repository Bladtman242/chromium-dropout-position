{
  "manifest_version": 3,
  "name": "dropout-progress",
  "version": "1.2.0",
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "icon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.dropout.tv/*"
      ],
      "js": ["player.js", "dropout-position.js"]
    }
  ]
}
